<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/HomeFragment">

    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.akshay.phonepetestapplication.views.ui.fragments.HomeFragment"
        android:label="@string/home_fragment"
        tools:layout="@layout/home_fragment">

        <action
            android:id="@+id/action_HomeFragment_to_BottomSheetFragment"
            app:destination="@id/BottomSheetFragment" />
    </fragment>
    <dialog
        android:id="@+id/BottomSheetFragment"
        android:name="com.akshay.phonepetestapplication.views.ui.fragments.BottomSheetFragment"
        android:label="@string/Bottom_Sheet"
        tools:layout="@layout/bottom_sheet_fragment" />
</navigation>